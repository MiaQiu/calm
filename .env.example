# API Keys - Copy this file to .env and add your actual keys

# Deepgram API Key (for fast speech-to-text)
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# Anthropic API Key (for Claude AI chat)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# ElevenLabs API Key (for text-to-speech)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# ElevenLabs Voice ID (optional - defaults to Bella voice)
# Find voice IDs at: https://api.elevenlabs.io/v1/voices
ELEVENLABS_VOICE_ID=EXAVITQu4vr4xnSDxMaL

# Alternative: Anthropic API Key (if using Claude instead of GPT-4)
# ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Server Port
PORT=3000
